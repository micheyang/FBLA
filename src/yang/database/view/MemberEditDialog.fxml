<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="yang.database.view.MemberEditDialogController">
   <children>
      <GridPane layoutX="25.0" layoutY="46.0" prefHeight="150.0" prefWidth="550.0">
